<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toBottomOf="@+id/personActivity_originImage">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/personActivity_Self1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/colorWhile">
        <ImageView
            android:id="@+id/personActivity_reclerView_AccountImage"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/ic_perm_identity_black_24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/personActivity_messageRecyclerView_AccountnameKey"
            android:layout_width="70dp"
            android:layout_height="35dp"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            android:gravity="center_vertical|left"
            android:text="用户名"
            android:textSize="18dp"
            android:ellipsize="end"
            android:singleLine="true"
            android:textColor="@color/colorgray"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/personActivity_reclerView_AccountImage"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/personActivity_messageRecyclerView_AccountnameValue"
            android:layout_width="60dp"
            android:layout_height="35dp"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            android:gravity="center_horizontal|center_vertical"
            android:text="林沫"
            android:textSize="18dp"
            android:textColor="@color/colorgray"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/personActivity_messageRecyclerView_AccounttoLeft"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/personActivity_messageRecyclerView_AccounttoLeft"
            android:layout_width="30dp"
            android:layout_height="35dp"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            android:gravity="center_vertical|left"
            android:src="@drawable/ic_right_arrow"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
